#############
# train classifier
#############
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -6class true -useReducedCategory true -2feat true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/2feat_all.ser.gz &
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -6class true -useReducedCategory true -2feat true -revised true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/revised_all.ser.gz &
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -6class true -useReducedCategory true -2feat true -revised true -ngram true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/ngram_all.ser.gz &
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -6class true -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/lastcharN_all.ser.gz &
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -6class true -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/lastcharNgram_all.ser.gz &
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -6class true -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -pword true -percentage true -ciLin true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/ciLinNoPath1st_all.ser.gz &
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -6class true -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -pword true -percentage true -ciLin true -nonOracleTree true -topicality true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/topicality_all.ser.gz &
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -6class true -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -pword true -percentage true -ciLin true -nonOracleTree true -topicality true -length true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/length_all.ser.gz &

#############
# train classifier (5class)
#############
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -useReducedCategory true -2feat true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/2feat_all.5class.ser.gz
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -useReducedCategory true -2feat true -revised true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/revised_all.5class.ser.gz
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -useReducedCategory true -2feat true -revised true -ngram true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/ngram_all.5class.ser.gz
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/lastcharN_all.5class.ser.gz
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/lastcharNgram_all.5class.ser.gz
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -pword true -percentage true -ciLin true -nonOracleTree true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/ciLinNoPath1st_all.5class.ser.gz
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -pword true -percentage true -ciLin true -nonOracleTree true -topicality true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/topicality_all.5class.ser.gz
time java -Xmx1g mt.translationtreebank.ClassifyingExperiment -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -pword true -percentage true -ciLin true -nonOracleTree true -topicality true -length true -trainAll true -writeClassifier projects/mt/src/mt/translationtreebank/report/nonoracle/length_all.5class.ser.gz


#######################
# 5 & 6 class, Multi Pass Experiments
#######################
time java -Xmx1g mt.translationtreebank.MultiPassClassifyingExperiment -splits 10 -passes 5 -useReducedCategory true -2feat true -nonOracleTree true > projects/mt/src/mt/translationtreebank/report/nonoracle/Multipass.2feat.report &
time java -Xmx1g mt.translationtreebank.MultiPassClassifyingExperiment -splits 10 -passes 5 -useReducedCategory true -2feat true -revised true -nonOracleTree true > projects/mt/src/mt/translationtreebank/report/nonoracle/Multipass.revised.report &
time java -Xmx1g mt.translationtreebank.MultiPassClassifyingExperiment -splits 10 -passes 5 -useReducedCategory true -2feat true -revised true -ngram true -nonOracleTree true  > projects/mt/src/mt/translationtreebank/report/nonoracle/Multipass.ngram.report &
time java -Xmx1g mt.translationtreebank.MultiPassClassifyingExperiment -splits 10 -passes 5 -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -nonOracleTree true > projects/mt/src/mt/translationtreebank/report/nonoracle/Multipass.lastcharN.report &
time java -Xmx1g mt.translationtreebank.MultiPassClassifyingExperiment -splits 10 -passes 5 -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -nonOracleTree true > projects/mt/src/mt/translationtreebank/report/nonoracle/Multipass.lastcharNgram.report &
time java -Xmx1g mt.translationtreebank.MultiPassClassifyingExperiment -splits 10 -passes 5 -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -percentage true -pword true -ciLin true -nonOracleTree true > projects/mt/src/mt/translationtreebank/report/nonoracle/Multipass.ciLinNoPath1st.report &
time java -Xmx1g mt.translationtreebank.MultiPassClassifyingExperiment -splits 10 -passes 5 -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -percentage true -pword true -ciLin true -topicality true -nonOracleTree true > projects/mt/src/mt/translationtreebank/report/nonoracle/Multipass.topicality.report &
time java -Xmx1g mt.translationtreebank.MultiPassClassifyingExperiment -splits 10 -passes 5 -useReducedCategory true -2feat true -revised true -ngram true -lastcharN true -lastcharNgram true -percentage true -pword true -ciLin true -topicality true -length true -nonOracleTree true > projects/mt/src/mt/translationtreebank/report/nonoracle/Multipass.length.report &

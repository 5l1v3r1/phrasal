# cat /scr/nlp/data/gale/MichelMTsystem/mt-experiments/zh-en.dep/bitrees/ctb/ctb-full/*zh.snt > ctb.zh.snt
# cat /scr/nlp/data/gale/MichelMTsystem/mt-experiments/zh-en.dep/bitrees/ctb/ctb-grow-diag/* > ctb.grow-diag
# cat /scr/nlp/data/gale/MichelMTsystem/mt-experiments/zh-en.dep/bitrees/ctb/ctb-full/*zh.trees > ctb.zh.trees

time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus ctb.zh.snt -eCorpus ctb.en.snt -align ctb.grow-diag > discrimreorder/baseline.ctb.out
time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus ctb.zh.snt -eCorpus ctb.en.snt -align ctb.grow-diag -src_j -tgt_i -window 0 > discrimreorder/zens_d0.ctb.out
time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus ctb.zh.snt -eCorpus ctb.en.snt -align ctb.grow-diag -src_j -tgt_i -window 1 > discrimreorder/zens_d1.ctb.out
time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus ctb.zh.snt -eCorpus ctb.en.snt -align ctb.grow-diag -src_j -tgt_i -src_j_src_j2 -src_j_tgt -window 1 > discrimreorder/word_conj.ctb.out
time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus ctb.zh.snt -eCorpus ctb.en.snt -align ctb.grow-diag -src_j -tgt_i -src_j_src_j2 -src_j_tgt -window 1 -head_dep_dir -fParse ctb.zh.trees > discrimreorder/wconj_path.ctb.out
time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus ctb.zh.snt -eCorpus ctb.en.snt -align ctb.grow-diag -src_j -tgt_i -window 1 -head_dep_dir -fParse ctb.zh.trees > discrimreorder/path.ctb.out

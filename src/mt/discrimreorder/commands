time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.zh -eCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.en -align /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/withBerkeleyAligner/alignment/model.thread4/aligned.grow-diag > discrimreorder/baseline.out
time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.zh -eCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.en -align /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/withBerkeleyAligner/alignment/model.thread4/aligned.grow-diag -src_j -tgt_i -window 0 > discrimreorder/zens_d0.out
time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.zh -eCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.en -align /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/withBerkeleyAligner/alignment/model.thread4/aligned.grow-diag -src_j -tgt_i -window 1 > discrimreorder/zens_d1.out
time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.zh -eCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.en -align /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/withBerkeleyAligner/alignment/model.thread4/aligned.grow-diag -src_j -tgt_i -src_j_src_j2 -src_j_tgt -window 1 > discrimreorder/word_conj.out
time java -mx15g mt.discrimreorder.ReorderingClassifier -fCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.zh -eCorpus /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/TM/corpus/corpus.en -align /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/withBerkeleyAligner/alignment/model.thread4/aligned.grow-diag -src_j -tgt_i -window 1 -head_dep_dir -fParse /scr/nlp/data/gale2/GrammaticalTrainingSubsetEval/withBerkeleyAligner/corpus.parse/corpus.zh.out > discrimreorder/path.out

